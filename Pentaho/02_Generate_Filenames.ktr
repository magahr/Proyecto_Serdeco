    <sql>SELECT 
    p.fecha_corte,
    p.estatus,
    cr.cod_serdeco,
    s.cod_servicio,
    pr.cod_producto
FROM periodo_procesado p
INNER JOIN codigo_referencial cr ON p.cod_serdeco = cr.cod_serdeco
INNER JOIN servicio s ON cr.id_servicio = s.id
CROSS JOIN producto pr
WHERE p.estatus = true </sql>
